<?xml version="1.0" encoding="utf-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="actor" class="com.stackroute.domain.Actor">
        <constructor-arg name="name" value="ramya"></constructor-arg>
        <constructor-arg name="gender" value="female"></constructor-arg>
        <constructor-arg name="age" value="23"></constructor-arg>
    </bean>
    <bean id="actor1" class="com.stackroute.domain.Actor">
        <constructor-arg type="String" value="abc"></constructor-arg>
        <constructor-arg type="String" value="male"></constructor-arg>
        <constructor-arg type="int" value="24"></constructor-arg>
    </bean>
    <bean id="actor2" class="com.stackroute.domain.Actor">
        <constructor-arg index="0" value="xyz"></constructor-arg>
        <constructor-arg index="1" value="female"></constructor-arg>
        <constructor-arg index="2" value="25"></constructor-arg>
    </bean>
    <bean id="movie" class="com.stackroute.domain.Movie" autowire="byName">

    </bean>
    <!--    <bean id="movie2" class="com.stackroute.domain.Movie">-->
    <!--        <constructor-arg name="actor" ref="actor"></constructor-arg>-->
    <!--    </bean>-->
    <bean id="beanlifecycledemobean" class="com.stackroute.BeanLifecycleDemoBean" init-method="customInit"
          destroy-method="customDestroy">
    </bean>
    <bean class="com.stackroute.BeanPostProcessorDemoBean"></bean>


</beans>